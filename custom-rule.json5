    'com.eg.android.AlipayGphone.AlipayLogin'
  ],
  
  // 规则列表
  rules: [
    {
      // 规则键值
      key: 0,
      
      // 规则描述
      desc: '点击返回按钮关闭访问被拒绝提示',
      
      // 匹配条件（使用逻辑AND）
      matches: [
        // 标题必须是"提示"
        "[id='com.alipay.mobile.antui:id/title_text'][text='提示']",
        // 内容必须是"访问被拒绝"
        "[id='com.alipay.android.phone.thirdparty:id/title'][text='访问被拒绝']"
      ],
      
      // 执行动作
      action: 'click',
      
      // 动作节点
      actionNodes: [
        {
          // 点击返回按钮
          matches: "[id='com.alipay.mobile.antui:id/back_button'][desc='返回']",
          // 点击位置：中心
          position: 'center'
        }
      ],
      
      // 是否截图（用于调试）
      snapshot: true,
      
      // 执行优先级（可选）
      // order: 0,
      
